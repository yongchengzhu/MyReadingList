<% include ./partials/header.ejs %>

<div class="ui container main">
    <table class="ui selectable celled table">
        <thead><tr>
            <th>Title</th>
            <th>Last Read</th>
            <th>Genre</th>
            <th>Rating</th>
            <th>Author</th>
        </tr></thead>
        <tbody>
        <% lists.forEach(function(list) { %>
            <tr>
                <td><a href="/lists/<%=list._id%>"><%= list.title %></a></td>
                <td><%= list.lastRead %></td>
                <td><%= list.genre %></td>
                <td><%= list.rating %></td>
                <td><%= list.author %></td>
            </tr>
        <% }); %>
        </tbody>
    </table>
</div>

<% include ./partials/footer.ejs %>